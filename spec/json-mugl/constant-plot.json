{
  "window": {
    "border": 0,
    "padding": 0,
    "margin": 0
  },
  "plotarea": {
    "marginbottom": 50,
    "margintop": 10,
    "marginleft": 60,
    "marginright": 25
  },
  "horizontalaxis": {
    "id": "x",
    "type": "datetime",
    "min": "200601",
    "max": "201207",
    "linewidth": 0,
    "tickmin": -4,
    "tickmax": 0,
    "highlightstyle": "labels",
    "title": {
      "text": "Month/Year",
      "anchor": [0, 5],
      "position": [-15, 0]
    },
    "labels": {
      "label": [
        {
          "format": "%Y",
          "spacing": ["100Y", "50Y", "20Y", "10Y", "5Y", "2Y", "1Y"]
        },
        {
          "format": "%n %y",
          "spacing": ["6M", "3M", "2M", "1M"]
        }
      ]
    },
    "grid": {
      "color": "0xeeeeee"
    }
  },
  "verticalaxis": {
    "id": "y",
    "min": -4.5,
    "max": 4.5,
    "linewidth": 0,
    "tickmin": -4,
    "tickmax": 0,
    "highlightstyle": "labels",
    "title": {
      "text": "Some Constant Plots",
      "anchor": [0, -4],
      "position": [-15, 0],
      "angle": 90
    },
    "labels": {
      "label": [
        {
          "format": "%1d",
          "spacing": [20, 10, 5, 2, 1],
          "densityfactor": 0.5
        },
        {
          "format": "%.1f",
          "spacing": [0.5, 0.2, 0.1],
          "densityfactor": 0.5
        }
      ]
    },
    "grid": {
      "color": "0xeeeeee"
    }
  },
  "plots": [
    {
      "style": "line",
      "options": {
        "linecolor": "0x999999",
        "linewidth": "1"
      },
      "verticalaxis": {
        "y": "0"
      },
      "legend": false
    },
    {
      "style": "line",
      "options": {
        "linecolor": "0x990000",
        "linewidth": "1"
      },
      "verticalaxis": {
        "y": "2"
      },
      "legend": false
    },
    {
      "style": "line",
      "options": {
        "linecolor": "0x000099",
        "linewidth": "3"
      },
      "verticalaxis": {
        "y": "-2"
      },
      "legend": false
    }
  ],
  "legend": {
    "visible": "false"
  }
}