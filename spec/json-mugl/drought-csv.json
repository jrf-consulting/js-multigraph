{
  "window": {
    "margin": 0,
    "padding": 5,
    "border": 5,
    "bordercolor": "0x000000"
  },
  "plotarea": {
    "margintop": 10,
    "marginleft": 38,
    "marginbottom": 35,
    "marginright": 35
  },
  "horizontalaxis": {
    "id": "time",
    "type": "datetime",
    "min": "1900",
    "max": "2014",
    "length": "1",
    "base": [
      -1,
      -1
    ],
    "anchor": -1,
    "labels": {
      "label": [
        {
          "spacing": [
            "500Y",
            "100Y",
            "50Y",
            "20Y",
            "10Y",
            "5Y",
            "2Y",
            "1Y"
          ],
          "format": "%Y"
        },
        {
          "spacing": [
            "6M",
            "3M",
            "2M",
            "1M"
          ],
          "format": "%n %Y"
        }
      ]
    }
  },
  "verticalaxis": {
    "id": "pdsi",
    "type": "number",
    "min": -7,
    "max": 7,
    "length": "1",
    "title": {
      "text": "Palmer Drought Serverity Index",
      "angle": 90,
      "anchor": [
        0,
        -1
      ],
      "position": [
        -30,
        0
      ]
    }
  },
  "plot": {
    "horizontalaxis": {
      "time": "time"
    },
    "verticalaxis": {
      "pdsi": "pdsi"
    },
    "options": {
      "linecolor": "0x000000",
      "fillcolor": [
        {
          "value": "0xfe9b00",
          "max": "-4"
        },
        {
          "value": "0xfece00",
          "min": "-4",
          "max": "-3"
        },
        {
          "value": "0xffff33",
          "min": "-3",
          "max": "-2"
        },
        {
          "value": "0xdddddd",
          "min": "-2",
          "max": "2"
        },
        {
          "value": "0x9cff89",
          "min": "2",
          "max": "3"
        },
        {
          "value": "0x69cd52",
          "min": "3",
          "max": "4"
        },
        {
          "value": "0x109a00",
          "min": "4"
        }
      ],
      "barbase": "0",
      "barwidth": "1M",
      "baroffset": "0",
      "hidelines": "10"
    },
    "style": "bar"
  },
  "data": {
    "adapter": "drought",
    "variables": [
      {
        "column": 0,
        "id": "time",
        "type": "datetime"
      },
      {
        "column": 1,
        "id": "pdsi",
        "type": "number",
        "missingvalue": "-99",
        "missingop": "le"
      }
    ],
    "csv": {
      "location": "drought.csv"
    }
  }
}